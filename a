MainSection:NewButton("Custom Roblox Editor", "Customize Roblox Settings", function()
    loadstring(game:HttpGet("https://eternityhub.xyz/HostStuff/JayzRoblox.lua"))()
end)


                -- MAIN
local Main = Window:NewTab("Fling Scripts")
